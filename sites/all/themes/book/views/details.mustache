{{>header}}
<div class="content main-content container">
	<ol class="breadcrumb">
		<li><a href="/">{{#labels}}{{index}}{{/labels}}</a></li>
		<li><a href="/book">{{#labels}}{{land_book}}{{/labels}}</a></li>
		<li><a href="/book/countries">{{#labels}}{{countries}}{{/labels}}</a></li>
		<li>
			<a id="continent-link" href="regions/{{#info}}{{#region}}{{un_code}}{{/region}}{{/info}}">
				{{#info}}{{#region}}{{name}}{{/region}}{{/info}}
			</a>
		</li>
		<li><a href="/book/countries/{{#info}}{{iso3}}{{/info}}">{{#info}}{{name}}{{/info}}</a></li>
		<li class="active">{{#labels}}{{detail}}{{/labels}}</li>
	</ol>
	<div class="row">

	</div>
	<div class="row">
		<div class="col-sm-9">
			<h1 class="country-name">
				<span>{{#labels}}{{all_available_indicators}}{{/labels}} {{#info}}{{name}}{{/info}}</span>
				{{#selectors}}
					{{#topics}}
						<h2 class="section"><span>{{name}}</span></h2>
						<table class="table table-striped">
							<thead>
								<tr>
									<th>{{#labels}}{{indicator}}{{/labels}}</th>
									<th>{{#labels}}{{value}}{{/labels}}</th>
									<th>{{#labels}}{{last_updated}}{{/labels}}</th>
									<th>{{#labels}}{{source}}{{/labels}}</th>
								</tr>
							</thead>
							<tbody>
								{{#indicators}}
									<tr>
										<td>{{name}}</td>
										<td>{{value}}</td>
										<td>{{last_update}}</td>
										<td>{{source}}</td>
									</tr>
								{{/indicators}}
							</tbody>
						</table>
					{{/topics}}
				{{/selectors}}
			</h1>
		</div>
		<div class="col-sm-3">
			<h2 class="section"><span>{{#labels}}{{specific_country}}{{/labels}}</span></h2>
			<select id="country-select" class="form-control">
				<option disabled="disabled" selected="true">{{#labels}}{{select_one}}{{/labels}}</option>
				{{#selectors}}
					{{#countries}}
					<option value="{{iso3}}">{{name}}</option>
				{{/countries}}
			{{/selectors}}
			</select>
			<a href="" class="more-info">{{#labels}}{{more_about_areas_and_countries}}{{/labels}}</a>
			<a href="
			/sparql?default-graph-uri=http%3A%2F%2Fbook.landportal.org&query=PREFIX+rdf%3A+%3Chttp%3A%2F%2Fwww.w3.org%2F1999%2F02%2F22-rdf-syntax-ns%23%3E%0D%0APREFIX+cex%3A+%3Chttp%3A%2F%2Fpurl.org%2Fweso%2Fcomputex%2Fontology%23%3E%0D%0APREFIX+lbonto%3A+%3Chttp%3A%2F%2Fpurl.org%2Fweso%2Flandbook%2Fontology%23%3E%0D%0APREFIX+qb%3A+%3Chttp%3A%2F%2Fpurl.org%2Flinked-data%2Fcube%23%3E%0D%0APREFIX+dcterms%3A+%3Chttp%3A%2F%2Fpurl.org%2Fdc%2Fterms%2F%3E%0D%0APREFIX+time%3A+%3Chttp%3A%2F%2Fwww.w3.org%2F2006%2Ftime%23%3E%0D%0A%0D%0A%0D%0ASELECT+%3Fiso3%2C+%3Find_name%2C+%3Fvalue%2C+%3Fstart_time%2C+%3Fend_time%2C+%3Fissued+WHERE%0D%0A%0D%0A%7B%0D%0A+++%3Fcountry+rdf%3Atype+cex%3AArea%3B%0D%0A++++++++++++lbonto%3Aiso3+%3Fiso3%3B%0D%0A++++++++++++lbonto%3Aiso3+%22{{#info}}{{iso3}}{{/info}}%22.++%23+Substitute+%22{{#info}}{{iso3}}{{/info}}%22+by+the+desired+iso3%0D%0A%0D%0A+++%3Fobs+cex%3Aref-area+%3Fcountry%3B%0D%0A++++++++rdf%3Atype+qb%3AObservation%3B%0D%0A++++++++cex%3Avalue+%3Fvalue%3B%0D%0A++++++++dcterms%3Aissued+%3Fissued%3B%0D%0A++++++++cex%3Aref-time+%3Fa_time%3B%0D%0A++++++++cex%3Aref-indicator+%3Fa_indicator.%0D%0A%0D%0A+++%3Fa_indicator+rdf%3Atype+cex%3AIndicator+%3B%0D%0A++++++++++++++++rdfs%3Alabel+%3Find_name+.%0D%0A%0D%0A+++%3Fa_time+rdf%3Atype+time%3ADateTimeInterval+%3B%0D%0A+++++++++++time%3AhasBeginning+%3Fa_beginning_time+%3B%0D%0A+++++++++++time%3AhasEnd+%3Fan_end_time+.%0D%0A%0D%0A++++%3Fa_beginning_time+rdf%3Atype+time%3AInstant+%3B%0D%0A++++++++++++++++++++++time%3AinXSDDateTime+%3Fstart_time.%0D%0A%0D%0A++++%3Fan_end_time+rdf%3Atype+time%3AInstant+%3B%0D%0A+++++++++++++++++time%3AinXSDDateTime+%3Fend_time+.%0D%0A%0D%0A%0D%0A%7D+ORDER+BY+ASC%28%3Find_name%29&format=application%2Fsparql-results%2Bjson&timeout=0&debug=on
			">
			<button class="btn data-button">
				{{#labels}}{{download_all_data}}{{/labels}} {{#info}}{{name}}{{/info}}
			</button>
			</a>
			<button class="btn data-button">
				{{#labels}}{{consult_library}}{{/labels}} {{#info}}{{name}}{{/info}}
				<p><strong>({{#labels}}{{coming_soon}}{{/labels}})</strong></p>
			</button>
			<a href="/search/site/{{#info}}{{name}}{{/info}}">
				<button class="btn data-button-dark">
						{{#labels}}{{debate_with_others_about}}{{/labels}}
						{{#info}}{{name}}{{/info}}
				</button>
			</a>
		</div>
	</div>
</div>
{{>footer}}
